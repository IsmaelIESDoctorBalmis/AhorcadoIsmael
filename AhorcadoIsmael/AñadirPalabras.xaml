<Window x:Class="AhorcadoIsmael.AñadirPalabras"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AhorcadoIsmael"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AñadirPalabras" Height="200" Width="250" MinHeight="200" MaxHeight="200" MinWidth="250" MaxWidth="250">

    <StackPanel>


        <TextBox x:Name="palabraAñadirTextBox" Width="100px" Margin="20">
            
        </TextBox>


        <Button x:Name="palabraAñadirButton" Width="100px">Añadir palabra</Button>

        <Button x:Name="volverJuegoButton" Width="100px" Margin="20" Click="volverJuegoButton_Click">Volver al juego</Button>
        
        
    </StackPanel>
    
    
    
    
    
</Window>
